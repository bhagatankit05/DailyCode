<body>
    <div class="w-full min-h-screen bg-zinc-900 p-10 text-white">
        <h1>Create User</h1>
        <form method="post" action="/create">
            <input class="px-3 py-2 rounded-md text-white border-2 border-zinc-800" type="email" name="email" placeholder="email">
            <input class="px-3 py-2 rounded-md  text-white border-2 border-zinc-800" type="text" name="name" placeholder="username">
            <input class="px-3 py-2 rounded-md  text-white border-2 border-zinc-800" type="password" name="password" placeholder="password">
            <input class="px-3 py-2 rounded-md  text-white border-2 border-zinc-800" type="number" name="age" placeholder="age">
            <input class="bg-blue-600 px-4 py-2 rounded-md" type="submit" value="Create User">
             
        </form>
        </form>

        </form>
    </div>
